<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lx.dao.TapplyDao">
    <select id="tappSelect" parameterType="java.util.HashMap" resultType="java.util.HashMap">
        select * from l_travel_apply;
    </select>
    <insert id="tappinsert" parameterType="java.util.HashMap" >
        insert into l_travel_apply(tapp_name,taap_cat,taap_licant,taap_data,taap_time,taap_appt,taap_state,taap_sdata,taap_rea)
        values(#{tapp_name},#{taap_cat},#{taap_licant},#{taap_data},#{taap_time},#{taap_appt},#{taap_state},#{taap_sdata},#{taap_rea})
    </insert>
</mapper>