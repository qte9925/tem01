<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lx.dao.TregDao">
    <select id="tselect" parameterType="java.util.HashMap" resultType="java.util.HashMap">
        select * from l_travel_register;
    </select>
    <select id="tselect01" parameterType="java.util.HashMap" resultType="java.util.HashMap">
        select * from l_travel_register where reg_id=#{reg_id};
    </select>
    <update id="tupdate" parameterType="java.util.HashMap">
        update l_travel_register set reg_bem=#{reg_bem},reg_type=#{reg_type},reg_path=#{reg_path},reg_name=#{reg_name},
        reg_baes=#{reg_baes},reg_end=#{reg_end},reg_zijin=#{reg_zijin},reg_jiaojie=#{reg_jiaojie},reg_qk=#{reg_qk} where reg_id=#{reg_id};
    </update>
    <delete id="tdelete" parameterType="java.util.HashMap">
          delete from l_travel_register where reg_id=#{reg_id};
    </delete>
    <insert id="tinsert" parameterType="java.util.HashMap">
        insert into l_travel_register(reg_bem,reg_type,reg_path,reg_name,reg_baes,reg_end,reg_zijin,reg_jiaojie,reg_qk)
        values(#{reg_bem},#{reg_type},#{reg_path},#{reg_name},#{reg_baes},#{reg_end},#{reg_zijin},#{reg_jiaojie},#{reg_qk})
    </insert>

</mapper>
